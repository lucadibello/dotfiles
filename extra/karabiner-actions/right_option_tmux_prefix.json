{
  "description": "Map right option to tmux prefix (Control+B)",
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "right_option",
        "modifiers": {
          "optional": [
            "any"
          ]
        }
      },
      "to": [
        {
          "key_code": "b",
          "modifiers": [
            "left_control"
          ]
        }
      ]
    }
  ]
}
